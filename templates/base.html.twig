<!DOCTYPE html>
<html>
	<head>
		<meta charset="UTF-8">
		<title>
			{% block title %}Ohshiwaddup!
			{% endblock %}
		</title>
	
		{% block stylesheets %}
			{{ encore_entry_link_tags('app') }}
		{% endblock %}
	</head>
	<body>
		<div class="container">
			{% include 'partials/menu.html.twig' %}
			<div class="row">
				<div class="col-md-4">
					{% block sidebar %}Sidebar{% endblock %}
				</div>
				<div class="col-md-8">
				{% block body %}{% endblock %}
				</div>
			</div>
		</div>
		{% block javascripts %}
			{{ encore_entry_script_tags('app') }}
		{% endblock %}
	</body>
</html>
